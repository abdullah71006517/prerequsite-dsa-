#include <iostream>
#include <vector>
using namespace std;
int main() {
    // Step 1: Create a vector of 5 integers
    vector<float> vec;//3

    for(int i=0;i<3;i++){
           float input;
           cin>>input;//5   //10
           //vector name.push_back(valu)
         vec.push_back(input);//vector input stl fucation
        // vec[i]=input;//vec[0]=5  vec[1]=10;
    }

   for(int i=0;i<3;i++){
         //  int input;
          // cin>>input;//5   //10
           //vector name.push_back(valu)
        //   vec.push_back(input);//vector input stl fucation
         //vec[0]=5  vec[1]=10;
         cout<<vec[i]<<endl;
    }


  // vec.resize(3);//call the fucation

   // cout << "Resized vector values:" << std::endl;

    // for (data type  variable name : vector name )
    //
    //box
    //auto    =container datatype
    //ceil 2.5=3
    //floor2.5=2;
   for (auto val : vec) {// range based loop   // box        container(range) 1.5
        cout << val << " ";                          // current vale
    }


    return 0;
}

